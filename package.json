{
  "name": "checkweather-sg",
  "version": "1.0.0",
  "description": "Yet another weather app for Singapore",
  "main": "index.html",
  "scripts": {
    "parcel": "parcel",
    "start": "parcel index.html -d tmp",
    "build": "parcel build index.html",
    "deploy": "rm -rf dist && npm run build && cp {CNAME,favicon.ico} dist && gh-pages -d dist"
  },
  "keywords": [
    "weather",
    "rain",
    "rainfall",
    "singapore",
    "map"
  ],
  "author": "Lim Chee Aun",
  "license": "MIT",
  "dependencies": {
    "new-point-haversine": "~0.0.7"
  },
  "devDependencies": {
    "gh-pages": "~1.1.0",
    "parcel-bundler": "~1.7.0"
  }
}
