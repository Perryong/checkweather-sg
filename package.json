{
  "name": "checkweather-sg",
  "version": "1.0.0",
  "description": "Yet another weather app for Singapore",
  "main": "index.html",
  "scripts": {
    "parcel": "parcel",
    "start": "parcel index.html -d tmp --no-hmr",
    "build": "rm -rf dist && parcel build index.html",
    "deploy": "npm run build && cp {CNAME,favicon.ico} dist && gh-pages -d dist"
  },
  "keywords": [
    "weather",
    "rain",
    "rainfall",
    "singapore",
    "map"
  ],
  "author": "Lim Chee Aun",
  "license": "MIT",
  "dependencies": {
    "mapbox-gl": "~0.47.0"
  },
  "devDependencies": {
    "babel-preset-es2015-ie": "~6.7.0",
    "gh-pages": "~1.2.0",
    "parcel-bundler": "~1.9.7"
  }
}
